{
    "name": "servport",
    "version": "1.0.0",
    "description": "MERN requests-services portal",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node backend/server",
        "server": "nodemon backend/server",
        "client": "npm start --prefix frontend",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "data:import": "node backend/seeder",
        "data:destroy": "node backend/seeder -d",
        "populate": "concurrently \"node backend/populate/usersPopulate\" \"node backend/populate/servicesPopulate\" \"node backend/populate/requestsPopulate\""
    },
    "author": "Bogdan Mihalachi",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^6.0.0",
        "nodemon": "^2.0.7"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "colors": "^1.4.0",
        "config": "^3.3.6",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "express-fileupload": "^1.2.1",
        "express-handlebars": "^5.3.0",
        "express-mongo-sanitize": "^2.0.2",
        "express-rate-limit": "^5.2.6",
        "express-validator": "^6.10.0",
        "gravatar": "^1.8.1",
        "helmet": "^4.4.1",
        "hpp": "^0.2.3",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.12.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "node-geocoder": "^3.27.0",
        "nodemailer": "^6.5.0",
        "react-helmet": "^6.1.0",
        "request": "^2.88.2",
        "slugify": "^1.5.0",
        "uuid": "^8.3.2",
        "xss-clean": "^0.1.1"
    }
}